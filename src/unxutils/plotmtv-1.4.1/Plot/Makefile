install.local:: all $(PROGRAM)
	$(INSTALL) -c $(INSTPGMFLAGS)  $(PROGNAME) $(LOCALBINDIR)

all:: $(PROGNAME).exe

$(PROGNAME).exe: $(OBJS) $(DEPLIBS)
	$(RM) $@
	$(CCLINK) -o $@ $(LDOPTIONS) $(OBJS) $(LOCAL_LIBRARIES) $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

install:: $(PROGNAME).exe
	@if [ -d $(DESTDIR)$(BINDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS)  $(PROGNAME).exe $(DESTDIR)$(BINDIR)/$(PROGNAME).exe

install.man:: $(PROGNAME).man
	@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTMANFLAGS) $(PROGNAME).man $(DESTDIR)$(MANDIR)/$(PROGNAME).$(MANSUFFIX)

